node C: ./gossiper -UIPort=10002 -gossipPort=127.0.0.1:5002 -name=nodeC -peers=127.0.0.1:5003 -rtimer=10
node D: ./gossiper -UIPort=10003 -gossipPort=127.0.0.1:5003 -name=nodeD -peers=127.0.0.1:5002 -rtimer=10